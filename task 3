#include <stdio.h>

int power_function(int number, int power)
{
  int result = 1;

  for(int i = 0; i < power; i++)
  {
    result *=  number;
  }

return result;

}

int main(void) 
{
  int number;
  int power;

  printf("Введите число: ");
  scanf("%d", &number);

  printf("Введите степень: ");
  scanf("%d", &power);
  
  printf("%d", power_function(number, power));

}
